import numpy as np
import matplotlib.pyplot as plt

fig = plt.figure('3D Contour', figsize=(9, 6))
ax = plt.axes(projection = '3d')


x = np.linspace(-10, 10, 100)
y = np.linspace(-15, 15, 100)

X, Y = np.meshgrid(x, y)
Z = np.sin(X) + np.cos(Y)

ax.contour3D(X, Y, Z, 100, cmap = 'viridis')

ax.set_xlabel('x')
ax.set_ylabel('y')
ax.set_zlabel('z')

plt.show()
